import java.util.*;
import java.lang.Math;

public class Soal3 UAS {
    private static Scanner input = new Scanner(System.in);

    public static void main(String[] args) {
        int n, i, a, x;

        n = Integer.parseInt(input.nextLine());
        String[] nama = new String[n];
        int[] perangkat = new int[n];

        x = 0;
        total(x, n);
    }
    
    public static void total(int x, int n) {
        int total, a, i;
        int[] perangkat = new int[n];
        String[] nama = new String[n];

        total = 0;
        int jumlah;

        for (i = 0; i <= n - 1; i++) {
            jumlah = 0;
            nama[i] = input.nextLine();
            perangkat[i] = Integer.parseInt(input.nextLine());
            a = perangkat[i];
            int[] konsumsi = new int[a];
            int[] ket = new int[a];

            for (x = 0; x <= a - 1; x++) {
                konsumsi[x] = Integer.parseInt(input.nextLine());
                validasi(konsumsi, x);
                total = total + konsumsi[x];
                jumlah = jumlah + konsumsi[x];
                ket[x] = jumlah;
            }
            System.out.println(nama[i] + " = " + jumlah);
        }
        System.out.println("Total konsumsi kamar = " + total + " wh");
        if (total > 10000) {
            System.out.println("anda sudah melewati batas");
        } else {
            System.out.println("Konsumsi masih dalam batas wajar.");
        }
    }
    
    public static void validasi(int[] konsumsi, int x) {
        if (konsumsi[x] < 0) {
            System.out.println("INVALID");
            konsumsi[x] = Integer.parseInt(input.nextLine());
        }
    }
}
