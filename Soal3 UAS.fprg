<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="Soal3 UAS"/>
        <attribute name="authors" value="ASUS"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-12-17 04:45:06 PM"/>
